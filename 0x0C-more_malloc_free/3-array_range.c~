#include "main.h"
#include <stdlib.h>

/**
 * array_range - creates an array of integers
 * @min - minimum
 * @max - maximum
 * Return: pointer to new allocated space in memory
 */
int *array_range(int min, int max)
{
	int *ptr;
	int i;
	char *ptr;

	if (min > max)
		return (NULL);


	ptr = malloc(size * nmemb);
	if (ptr == NULL)
		return (NULL);


	for (i = 0; i < (int)(nmemb * size); i++)
	{
		ptr[i] = 0;
	}

	return (ptr);
}
